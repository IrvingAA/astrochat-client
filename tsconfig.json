{
  "extends": "./.quasar/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "allowJs": false,
    "strict": true,
    "verbatimModuleSyntax": true,
    "forceConsistentCasingInFileNames": true,
    "useDefineForClassFields": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitThis": true,
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ]
}
